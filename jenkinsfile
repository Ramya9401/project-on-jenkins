pipeline {
    agent any
    stages {
        stage('build') {
        steps {
          echo "building is done"
          isUnix("mvn clean")
        } 
        }
        stage('test') {
        steps {
          echo "testing is done"
          sh "mvn test"
       } 
        }
        stage('compile') {
        steps {sh "mvn clean"
          echo "compiling is done"
          sh "mvn compile"
       } 
        }
        stage('deploy') {
        steps {
          echo "deploying is done"
       } 
        }
    }
}
