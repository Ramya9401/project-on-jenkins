pipeline{
    agent any
    stages{
        stage('build')
        {
            steps{
                git 'https://github.com/Ramya9401/project-on-jenkins.git'
                
                sh 'mvn clean install'
            }
        }
        stage('build docker image')
        {
            steps{
                script{
                sh 'sudo -S docker build -t mummadiramya/mav-project:latest .'
                }
            }
            
        }
        stage('docker build')
        {
            steps{
                sh "sudo docker login -u="$mummadiramya" --password-stdin
                sh 'sudo docker push mummadiramya/mav-project:latest'
                
            }
        }
    }
}
